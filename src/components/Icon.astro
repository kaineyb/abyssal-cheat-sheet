---
import { type Icons } from "../data/_base";
import { getIcon } from "../data/icons";

interface Props {
  icon: Icons;
}

const { icon } = Astro.props;
---

<img
  title={icon}
  width="32px"
  height="32px"
  src={getIcon(icon)}
  alt={icon}
/>

<style scoped>
  img {
    /* outline: 1px solid red; */
  }
</style>
